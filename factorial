// factorial for
let factorial = 1;
let i;
for (i = 10; i >= 1; i--) {
    factorial *= i
}
console.log(factorial);    //3628800

--------
// factorial break
let factorial = 1   
let num = 10
while (true) {
    factorial *= num
    num--
    if (num === 1) break
}
console.log(factorial);    //3628800

-------
// factorial while
let factorial = 1
let num = 10
while (num >= 1) {
    factorial *= num
    num--
}
console.log(factorial)  //3628800
